{
  "name": "google-meet-quick-repray",
  "description": "you can quickly repray.",
  "version": "0.1.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://meet.google.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
